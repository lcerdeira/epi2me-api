{
  "name": "@metrichor/epi2me-api",
  "version": "3.0.1623",
  "license": "MPL-2.0",
  "repository": "git@github.com:nanoporetech/epi2me-api.git",
  "description": "API for communicating with the EPI2ME website(s)",
  "main": "dist/index.js",
  "types": "./src/types/index.d.ts",
  "module": "dist/index.es.js",
  "dependencies": {
    "@lifeomic/axios-fetch": "^1.4.2",
    "@types/socket.io-client": "^1.4.32",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.13",
    "apollo-link-context": "^1.0.19",
    "apollo-link-http": "^1.5.16",
    "aws-sdk": "^2.610.0",
    "axios": "^0.21.1",
    "core-js": "^3.6.4",
    "fdir": "^3.2.0",
    "fs-extra": "^8.1.0",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.1",
    "lodash": "^4.17.15",
    "proxy-agent": "^3.1.1",
    "rxjs": "^6.5.4",
    "save": "^2.4.0",
    "socket.io-client": "^2.3.0",
    "sqlite": "^3.0.3",
    "tunnel": "^0.0.6"
  },
  "devDependencies": {
    "@rollup/plugin-sucrase": "^3.0.0",
    "@types/bunyan": "^1.8.6",
    "@types/fs-extra": "^8.1.0",
    "@types/lodash": "^4.14.149",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.9.0",
    "@types/rollup": "^0.54.0",
    "@types/rollup-plugin-json": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "bunyan": "^1.8.12",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-defaults": "9.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-graphql": "^3.1.1",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.1",
    "lint-staged": "^10.0.4",
    "mocha": "^7.0.1",
    "mock-fs": "^4.11.0",
    "nyc": "^15.0.0",
    "prettier": "^1.18.2",
    "prettier-eslint": "^9.0.0",
    "rollup": "^1.30.1",
    "rollup-plugin-analyzer": "^3.2.1",
    "rollup-plugin-cpy": "^2.0.1",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-generate-package-json": "^3.1.3",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-license": "^0.13.0",
    "rollup-plugin-terser": "^5.2.0",
    "sinon": "^8.1.1",
    "tmp": "^0.1.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3",
    "xunit-file": "*"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "npm run fix-ts --fix",
      "git add --force"
    ],
    "*.{json,md,graphql}": [
      "prettier --write",
      "git add --force"
    ]
  },
  "scripts": {
    "build:version": "jq \".version=\\\"$(jq -r .version package.json | cut -d . -f 1-2).${PATCH:-$(date +%-H%M)}\\\"\" < package.json > package.json.tmp && mv package.json.tmp package.json",
    "lint-js": "eslint --ignore-path .eslintignore --ignore-pattern \"!**/.*\" .",
    "lint:errors": "eslint --quiet --ignore-path .eslintignore --ignore-pattern \"!**/.*\" .",
    "fix-js": "npm run lint-js --fix",
    "lint": "npm run lint-js",
    "deps": "npm ci",
    "clean:dist": "rm -rf dist",
    "clean:build": "rm -rf build && rm -rf dist/lib",
    "clean": "npm run clean:build && npm run clean:dist",
    "test": "TS_NODE_TRANSPILE_ONLY=true mocha --recursive --require ts-node/register test",
    "test:single": "TS_NODE_TRANSPILE_ONLY=true mocha --require ts-node/register test",
    "cover": "npm install && npm run lint && npx nyc --reporter=html --reporter=text mocha --recursive --require ts-node/register test",
    "build": "npm ci && npm run build:dist",
    "rollup:build": "npx rollup -c",
    "rollup:watch": "npx rollup -cw",
    "build:light": "npm run clean:dist && npm run rollup:build",
    "build:dist": "npm run build:version && npm run clean:dist && npm ci && npm run rollup:build",
    "ts:defs": "tsc --declaration --outDir ./types --emitDeclarationOnly"
  }
}
